package buses
public
	bus power_bus
	features
		provide : provides bus access;
		require : requires bus access;
	end power_bus;

	bus media_bus
	end media_bus;

	bus implementation media_bus.agv
	end media_bus.agv;

	bus implementation power_bus.agv
	end power_bus.agv;

	bus implementation media_bus.arr
	end media_bus.arr;

	bus implementation power_bus.arr
	end power_bus.arr;

	bus new_classifier
	end new_classifier;

end buses;