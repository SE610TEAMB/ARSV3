package buses
public
	bus power_bus
	features
		require : requires bus access;
	end power_bus;

	bus media_bus
	end media_bus;

	bus implementation media_bus.agv
	end media_bus.agv;

	bus implementation power_bus.agv
	end power_bus.agv;

	bus implementation media_bus.arr
	end media_bus.arr;

	bus implementation power_bus.arr
	end power_bus.arr;

	bus system_bus
	end system_bus;

	bus implementation system_bus.arr
	end system_bus.arr;

	bus implementation system_bus.agv
	end system_bus.agv;

end buses;